module Topl.Utils where

data SendStatus = New
                | Pending
                | Sent with
                    when   : Time
                    from   : Text
                    txHash : Optional Text
                | FailedToSend with
                    reason : Text
  deriving (Eq, Show)


data AssetCode =  AssetCode with 
    version : Int
    issuerAddress : Text
    shortName : Text